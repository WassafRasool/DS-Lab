#include <iostream>
using namespace std;
#define SIZE 5

class CQueue {
    int arr[SIZE], front, rear, count;
public:
    CQueue() { front = 0; rear = -1; count = 0; }
    bool isFull() { return count == SIZE; }
    bool isEmpty() { return count == 0; }
    void enqueue(int x) {
        if (isFull()) cout << "Full\n";
        else {
            rear = (rear + 1) % SIZE;
            arr[rear] = x;
            count++;
        }
    }
    void dequeue() {
        if (isEmpty()) cout << "Empty\n";
        else {
            cout << "Removed: " << arr[front] << endl;
            front = (front + 1) % SIZE;
            count--;
        }
    }
    void display() {
        if (isEmpty()) cout << "Empty\n";
        else {
            int i = front;
            for (int c = 0; c < count; c++) {
                cout << arr[i] << " ";
                i = (i + 1) % SIZE;
            }
            cout << endl;
        }
    }
};

int main() {
    CQueue q;
    q.enqueue(1);
    q.enqueue(2);
    q.enqueue(3);
    q.enqueue(4);
    q.display();
    q.dequeue();
    q.enqueue(5);
    q.display();
}
